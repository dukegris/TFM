            <xs:element ref="Seq-entry"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- DlFini -->
      <xs:element name="ID1server-back_fini">
        <xs:complexType/>
      </xs:element>
      <xs:element name="ID1server-back_gistate" type="xs:integer"/>
      <xs:element name="ID1server-back_ids">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Seq-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!--
 because hand crafted Seq-hist does not follow 
 same conventions 
-->
      <xs:element name="ID1server-back_gihist">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="ID1Seq-hist"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ID1server-back_girevhist">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="ID1Seq-hist"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ID1server-back_gotsewithinfo">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="ID1SeqEntry-info"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ID1server-back_gotblobinfo">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="ID1blob-info"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
</xs:element>

