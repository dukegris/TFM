<!--
**********************************
 requests

-->
<xs:element name="ID1server-request">
  <xs:complexType>
    <xs:choice>
<!-- DlInit -->
      <xs:element name="ID1server-request_init">
        <xs:complexType/>
      </xs:element>
<!-- get a gi given a Seq-id -->
      <xs:element name="ID1server-request_getgi">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Seq-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- given a gi, get the Seq-entry -->
      <xs:element name="ID1server-request_getsefromgi">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="ID1server-maxcomplex"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- DlFini -->
      <xs:element name="ID1server-request_fini">
        <xs:complexType/>
      </xs:element>
<!--get all Seq-ids of given gi -->
      <xs:element name="ID1server-request_getseqidsfromgi" type="xs:integer"/>
<!--get an historical list of gis  -->
      <xs:element name="ID1server-request_getgihist" type="xs:integer"/>
<!--get a revision history of gi -->
      <xs:element name="ID1server-request_getgirev" type="xs:integer"/>
<!--get a state of gi -->
      <xs:element name="ID1server-request_getgistate" type="xs:integer"/>
      <xs:element name="ID1server-request_getsewithinfo">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="ID1server-maxcomplex"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ID1server-request_getblobinfo">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="ID1server-maxcomplex"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
</xs:element>

<!--  Complexity stuff will be for ID1 -->
<xs:element name="ID1server-maxcomplex">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="ID1server-maxcomplex_maxplex">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Entry-complexities"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ID1server-maxcomplex_gi" type="xs:integer"/>
<!-- needed when you want to retrieve a given ent -->
      <xs:element name="ID1server-maxcomplex_ent" type="xs:integer" minOccurs="0"/>
<!-- satellite 0-id,1-dbEST -->
      <xs:element name="ID1server-maxcomplex_sat" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="Entry-complexities">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:integer">
        <xs:attribute name="value" use="optional">
          <xs:simpleType>
            <xs:restriction base="xs:string">
<!--
    entry	-  the "natural" entry for this (nuc-prot) 
    bioseq	-  only the bioseq identified
    bioseq-set	-  any seg-set it may be part of
    nuc-prot	-  any nuc-prot it may be part of
-->
              <xs:enumeration value="entry" ncbi:intvalue="0"/>
              <xs:enumeration value="bioseq" ncbi:intvalue="1"/>
              <xs:enumeration value="bioseq-set" ncbi:intvalue="2"/>
              <xs:enumeration value="nuc-prot" ncbi:intvalue="3"/>
              <xs:enumeration value="pub-set" ncbi:intvalue="4"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>

<xs:element name="ID1Seq-hist">
  <xs:complexType>
