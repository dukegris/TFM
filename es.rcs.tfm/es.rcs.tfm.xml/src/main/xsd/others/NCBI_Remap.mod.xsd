**********************************************************
 Replies from the server
  all replies contain the date/time stamp when they were executed
**********************************************************
-->
<xs:element name="Remap-reply">
  <xs:complexType>
    <xs:sequence>
<!-- the actual reply -->
      <xs:element name="Remap-reply_reply">
