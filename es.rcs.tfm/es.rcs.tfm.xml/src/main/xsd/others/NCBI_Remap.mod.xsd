        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Remap-dt"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- server version info -->
      <xs:element name="Remap-reply_server" type="xs:string"/>
<!-- possibly a message to the user -->
      <xs:element name="Remap-reply_msg" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="RMReply">
  <xs:complexType>
    <xs:choice>
<!-- if nothing can be returned -->
      <xs:element name="RMReply_error" type="xs:string"/>
<!-- result of actual remapping -->
      <xs:element name="RMReply_remap">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Remap-result"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!--
 all the builds that the server
 knows how to map this build to
-->
      <xs:element name="RMReply_maps-to-builds">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="RMReply_maps-to-builds_E" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!--
 all the builds that the server
 knows how to map to this build
-->
      <xs:element name="RMReply_maps-from-builds">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="RMReply_maps-from-builds_E" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- all builds that the server knows of -->
      <xs:element name="RMReply_all-builds">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
