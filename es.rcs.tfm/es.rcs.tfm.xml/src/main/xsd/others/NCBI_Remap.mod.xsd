<!--
***************************************
  Remap Request types
***************************************
****************************************
 The basic request wrapper leaves space for a version which
   allow the server to support older clients
 The tool parameter allows us to log the client types for
   debugging and tuning
****************************************
 a standard request
-->
<xs:element name="Remap-request">
  <xs:complexType>
    <xs:sequence>
<!-- the actual request -->
      <xs:element name="Remap-request_request">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="RMRequest"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- ASN1 spec version -->
      <xs:element name="Remap-request_version" type="xs:integer"/>
<!-- tool making request -->
      <xs:element name="Remap-request_tool" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- request types -->
<xs:element name="RMRequest">
  <xs:complexType>
    <xs:choice>
<!-- do the actual remapping -->
      <xs:element name="RMRequest_remap">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Remap-query"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- what builds can this be mapped to? -->
      <xs:element name="RMRequest_maps-to-builds" type="xs:string"/>
<!-- what builds can be mapped to this? -->
      <xs:element name="RMRequest_maps-from-builds" type="xs:string"/>
<!-- all the builds the server knows of -->
      <xs:element name="RMRequest_all-builds">
        <xs:complexType/>
      </xs:element>
    </xs:choice>
  </xs:complexType>
</xs:element>

<xs:element name="Remap-query">
  <xs:complexType>
    <xs:sequence>
<!-- build to map from -->
      <xs:element name="Remap-query_from-build" type="xs:string"/>
<!-- build to map to -->
      <xs:element name="Remap-query_to-build" type="xs:string"/>
<!-- the locations to remap -->
      <xs:element name="Remap-query_locs">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Seq-loc"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

