      <xs:element name="Entrez2-db-info_field-count" type="xs:integer"/>
      <xs:element name="Entrez2-db-info_fields">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Entrez2-field-info"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- number of link types -->
      <xs:element name="Entrez2-db-info_link-count" type="xs:integer"/>
      <xs:element name="Entrez2-db-info_links">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Entrez2-link-info"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Entrez2-db-info_docsum-field-count" type="xs:integer"/>
      <xs:element name="Entrez2-db-info_docsum-fields">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Entrez2-docsum-field-info"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- info about one field -->
<xs:element name="Entrez2-field-info">
  <xs:complexType>
    <xs:sequence>
<!-- the internal name -->
      <xs:element name="Entrez2-field-info_field-name">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Entrez2-field-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- short string suitable for menu -->
      <xs:element name="Entrez2-field-info_field-menu" type="xs:string"/>
<!-- longer, explanatory name -->
      <xs:element name="Entrez2-field-info_field-descr" type="xs:string"/>
<!-- number of terms in field -->
      <xs:element name="Entrez2-field-info_term-count" type="xs:integer"/>
      <xs:element name="Entrez2-field-info_is-date" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="Entrez2-field-info_is-numerical" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="Entrez2-field-info_single-token" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="Entrez2-field-info_hierarchy-avail" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="Entrez2-field-info_is-rangable" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="Entrez2-field-info_is-truncatable" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- info about one link -->
<xs:element name="Entrez2-link-info">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Entrez2-link-info_link-name">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Entrez2-link-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Entrez2-link-info_link-menu" type="xs:string"/>
      <xs:element name="Entrez2-link-info_link-descr" type="xs:string"/>
<!-- database it links to -->
      <xs:element name="Entrez2-link-info_db-to">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Entrez2-db-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- size of link data element     -->
      <xs:element name="Entrez2-link-info_data-size" type="xs:integer" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="Entrez2-docsum-field-type">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:integer">
        <xs:attribute name="value" use="optional">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="string" ncbi:intvalue="1"/>
              <xs:enumeration value="int" ncbi:intvalue="2"/>
              <xs:enumeration value="float" ncbi:intvalue="3"/>
              <xs:enumeration value="date-pubmed" ncbi:intvalue="4"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>

<xs:element name="Entrez2-docsum-field-info">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Entrez2-docsum-field-info_field-name" type="xs:string"/>
      <xs:element name="Entrez2-docsum-field-info_field-description" type="xs:string"/>
      <xs:element name="Entrez2-docsum-field-info_field-type">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Entrez2-docsum-field-type"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="Entrez2-boolean-reply">
  <xs:complexType>
    <xs:sequence>
<!-- records hit -->
      <xs:element name="Entrez2-boolean-reply_count" type="xs:integer"/>
<!-- if uids requested -->
      <xs:element name="Entrez2-boolean-reply_uids" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Entrez2-id-list"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- if parsed query requested -->
      <xs:element name="Entrez2-boolean-reply_query" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Entrez2-boolean-exp"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="Entrez2-docsum-list">
  <xs:complexType>
    <xs:sequence>
<!-- number of docsums -->
      <xs:element name="Entrez2-docsum-list_count" type="xs:integer"/>
      <xs:element name="Entrez2-docsum-list_list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Entrez2-docsum"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="Entrez2-docsum">
  <xs:complexType>
    <xs:sequence>
<!-- primary uid (gi, pubmedid) -->
      <xs:element name="Entrez2-docsum_uid" type="xs:integer"/>
      <xs:element name="Entrez2-docsum_docsum-data">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Entrez2-docsum-data"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="Entrez2-docsum-data">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Entrez2-docsum-data_field-name" type="xs:string"/>
      <xs:element name="Entrez2-docsum-data_field-value" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="Entrez2-term-list">
  <xs:complexType>
    <xs:sequence>
<!-- position of first term in list -->
      <xs:element name="Entrez2-term-list_pos" type="xs:integer"/>
<!-- number of terms in list -->
      <xs:element name="Entrez2-term-list_num" type="xs:integer"/>
      <xs:element name="Entrez2-term-list_list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Entrez2-term"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="Entrez2-term">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Entrez2-term_term" type="xs:string"/>
      <xs:element name="Entrez2-term_txid" type="xs:integer" minOccurs="0"/>
<!-- count of records with this term -->
      <xs:element name="Entrez2-term_count" type="xs:integer"/>
<!-- used for hierarchy only -->
      <xs:element name="Entrez2-term_is-leaf-node" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- for hierarchical index -->
<xs:element name="Entrez2-hier-node">
  <xs:complexType>
    <xs:sequence>
<!-- the official name -->
      <xs:element name="Entrez2-hier-node_cannonical-form" type="xs:string"/>
<!-- number of strings in lineage -->
      <xs:element name="Entrez2-hier-node_lineage-count" type="xs:integer"/>
<!-- strings up the lineage -->
      <xs:element name="Entrez2-hier-node_lineage" minOccurs="0">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Entrez2-term"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- number of children of this node -->
      <xs:element name="Entrez2-hier-node_child-count" type="xs:integer"/>
<!-- the children -->
      <xs:element name="Entrez2-hier-node_children">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Entrez2-term"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- used for hierarchy only -->
      <xs:element name="Entrez2-hier-node_is-ambiguous" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!--
*******************************************
 Links are returned in sets also using OCTET STRINGS
*******************************************
 set of links
-->
<xs:element name="Entrez2-link-set">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Entrez2-link-set_ids">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Entrez2-id-list"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- size of data elements -->
      <xs:element name="Entrez2-link-set_data-size" type="xs:integer" minOccurs="0"/>
<!-- coded scores -->
      <xs:element name="Entrez2-link-set_data" type="xs:hexBinary" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- all links from 1 uid -->
<xs:element name="Entrez2-link-count-list">
  <xs:complexType>
    <xs:sequence>
<!-- number of types of links -->
      <xs:element name="Entrez2-link-count-list_link-type-count" type="xs:integer"/>
      <xs:element name="Entrez2-link-count-list_links">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Entrez2-link-count"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- link count of one type -->
<xs:element name="Entrez2-link-count">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Entrez2-link-count_link-type">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Entrez2-link-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Entrez2-link-count_link-count" type="xs:integer"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

</xs:schema>
