      <xs:element name="Entrez2-reply_server" type="xs:string"/>
<!-- possibly a message to the user -->
      <xs:element name="Entrez2-reply_msg" type="xs:string" minOccurs="0"/>
<!-- history key for query -->
      <xs:element name="Entrez2-reply_key" type="xs:string" minOccurs="0"/>
<!-- history session cookie -->
      <xs:element name="Entrez2-reply_cookie" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

