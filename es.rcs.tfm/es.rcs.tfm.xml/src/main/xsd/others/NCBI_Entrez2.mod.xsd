        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Entrez2-boolean-element"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- date bounds -->
      <xs:element name="Entrez2-boolean-exp_limits" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Entrez2-limits"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

