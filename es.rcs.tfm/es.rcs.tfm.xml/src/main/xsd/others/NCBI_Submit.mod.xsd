<!-- deletions of entries -->
<xs:element name="Seq-submit">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Seq-submit_sub">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Submit-block"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Seq-submit_data">
        <xs:complexType>
          <xs:choice>
<!-- sequence(s) -->
            <xs:element name="Seq-submit_data_entrys">
              <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                  <xs:element ref="Seq-entry"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
<!-- annotation(s) -->
            <xs:element name="Seq-submit_data_annots">
              <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                  <xs:element ref="Seq-annot"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="Seq-submit_data_delete">
              <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                  <xs:element ref="Seq-id"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="Submit-block">
  <xs:complexType>
    <xs:sequence>
<!-- who to contact -->
      <xs:element name="Submit-block_contact">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Contact-info"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- citation for this submission -->
      <xs:element name="Submit-block_cit">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Cit-sub"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- hold until publish -->
      <xs:element name="Submit-block_hup" minOccurs="0">
        <xs:complexType>
          <xs:attribute name="value" use="optional" default="false">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- release by date -->
      <xs:element name="Submit-block_reldate" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Date"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Submit-block_subtype" minOccurs="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    new	-  new data
    update	-  update by author
    revision	-  3rd party (non-author) update
-->
                    <xs:enumeration value="new" ncbi:intvalue="1"/>
                    <xs:enumeration value="update" ncbi:intvalue="2"/>
                    <xs:enumeration value="revision" ncbi:intvalue="3"/>
                    <xs:enumeration value="other" ncbi:intvalue="255"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
<!-- tool used to make submission -->
      <xs:element name="Submit-block_tool" type="xs:string" minOccurs="0"/>
<!-- user supplied id for this submission -->
      <xs:element name="Submit-block_user-tag" type="xs:string" minOccurs="0"/>
<!-- user comments/advice to database -->
      <xs:element name="Submit-block_comment" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

