                          <xs:enumeration value="paired-end" ncbi:intvalue="13"/>
                          <xs:enumeration value="pcr" ncbi:intvalue="14"/>
                          <xs:enumeration value="qpcr" ncbi:intvalue="15"/>
                          <xs:enumeration value="read-depth" ncbi:intvalue="16"/>
                          <xs:enumeration value="roma" ncbi:intvalue="17"/>
                          <xs:enumeration value="rt-pcr" ncbi:intvalue="18"/>
                          <xs:enumeration value="sage" ncbi:intvalue="19"/>
                          <xs:enumeration value="sequence-alignment" ncbi:intvalue="20"/>
                          <xs:enumeration value="sequencing" ncbi:intvalue="21"/>
                          <xs:enumeration value="snp-array" ncbi:intvalue="22"/>
                          <xs:enumeration value="snp-genoytyping" ncbi:intvalue="23"/>
                          <xs:enumeration value="southern" ncbi:intvalue="24"/>
                          <xs:enumeration value="western" ncbi:intvalue="25"/>
                          <xs:enumeration value="optical-mapping" ncbi:intvalue="26"/>
                          <xs:enumeration value="other" ncbi:intvalue="255"/>
