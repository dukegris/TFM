                <xs:attribute name="value" use="required">
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:enumeration value="forward"/>
                      <xs:enumeration value="reverse"/>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
              </xs:complexType>
            </xs:element>
<!--
chromosome position as integer for
						sorting
-->
            <xs:element name="MapLoc_attlist_physMapInt" type="xs:integer" minOccurs="0"/>
<!--
nearest aligned position in 5' flanking sequence of
						snp
-->
            <xs:element name="MapLoc_attlist_leftFlankNeighborPos" type="xs:integer" minOccurs="0"/>
<!--nearest aligned position in 3' flanking sequence of snp -->
            <xs:element name="MapLoc_attlist_rightFlankNeighborPos" type="xs:integer" minOccurs="0"/>
<!--
nearest aligned position in 5' contig alignment of
						snp
-->
            <xs:element name="MapLoc_attlist_leftContigNeighborPos" type="xs:integer" minOccurs="0"/>
<!--
nearest aligned position in 3' contig alignment of
						snp
-->
            <xs:element name="MapLoc_attlist_rightContigNeighborPos" type="xs:integer" minOccurs="0"/>
<!--
number of Mismatched positions in this
						alignment
-->
            <xs:element name="MapLoc_attlist_numberOfMismatches" type="xs:integer" minOccurs="0"/>
<!--number of deletions in this alignment -->
            <xs:element name="MapLoc_attlist_numberOfDeletions" type="xs:integer" minOccurs="0"/>
<!--number of insetions in this alignment -->
            <xs:element name="MapLoc_attlist_numberOfInsertions" type="xs:integer" minOccurs="0"/>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="MapLoc_fxnSet" minOccurs="0">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="FxnSet"/>
