residue specified for protein at prot-loc
						location
-->
            <xs:element name="RsStruct_attlist_protResidue" type="xs:string" minOccurs="0"/>
<!--
alternative residue specified by variation
						sequence
-->
            <xs:element name="RsStruct_attlist_rsResidue" type="xs:string" minOccurs="0"/>
<!--GI of the structure neighbor -->
            <xs:element name="RsStruct_attlist_structGi" type="xs:integer" minOccurs="0"/>
<!--
position of the residue for the structure
						GI
-->
            <xs:element name="RsStruct_attlist_structLoc" type="xs:integer" minOccurs="0"/>
<!--
residue specified for protein at struct-loc
						location
-->
            <xs:element name="RsStruct_attlist_structResidue" type="xs:string" minOccurs="0"/>
          </xs:all>
        </xs:complexType>
      </xs:element>
<!--structure information for SNP -->
      <xs:element name="RsStruct_rsStruct">
        <xs:complexType/>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!--data for an individual submission to dbSNP -->
<xs:element name="Ss">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Ss_attlist">
        <xs:complexType>
          <xs:all>
<!--dbSNP accession number for submission -->
            <xs:element name="Ss_attlist_ssId" type="xs:integer"/>
<!--Tag for the submitting laboratory -->
            <xs:element name="Ss_attlist_handle" type="xs:string"/>
<!--dbSNP number for batch submission -->
            <xs:element name="Ss_attlist_batchId" type="xs:integer"/>
<!--submission (ss#) submitter ID -->
            <xs:element name="Ss_attlist_locSnpId" type="xs:string" minOccurs="0"/>
            <xs:element name="Ss_attlist_subSnpClass" minOccurs="0">
              <xs:complexType>
                <xs:attribute name="value" use="required">
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:enumeration value="snp"/>
                      <xs:enumeration value="in-del"/>
                      <xs:enumeration value="heterozygous"/>
                      <xs:enumeration value="microsatellite"/>
                      <xs:enumeration value="named-locus"/>
                      <xs:enumeration value="no-variation"/>
                      <xs:enumeration value="mixed"/>
                      <xs:enumeration value="multinucleotide-polymorphism"/>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
              </xs:complexType>
            </xs:element>
            <xs:element name="Ss_attlist_orient" minOccurs="0">
              <xs:complexType>
                <xs:attribute name="value" use="required">
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:enumeration value="forward"/>
                      <xs:enumeration value="reverse"/>
                    </xs:restriction>
                  </xs:simpleType>
