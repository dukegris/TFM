        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_real" type="xs:double"/>
      <xs:element name="Blast4-value_seq-align">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Seq-align"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_seq-id">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Seq-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_seq-loc">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Seq-loc"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_strand-type">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Blast4-strand-type"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_string" type="xs:string"/>
<!-- lists of scalar types -->
      <xs:element name="Blast4-value_big-integer-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Blast4-value_big-integer-list_E" type="xs:long"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_bioseq-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Bioseq"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_boolean-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Blast4-value_boolean-list_E">
              <xs:complexType>
                <xs:attribute name="value" use="required">
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:enumeration value="true"/>
                      <xs:enumeration value="false"/>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_cutoff-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Blast4-cutoff"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_integer-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Blast4-value_integer-list_E" type="xs:integer"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_matrix-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="PssmWithParameters"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_real-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Blast4-value_real-list_E" type="xs:double"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_seq-align-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Seq-align"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_seq-id-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Seq-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_seq-loc-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Seq-loc"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_strand-type-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Blast4-strand-type"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_string-list">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Blast4-value_string-list_E" type="xs:string"/>
          </xs:sequence>
