<xs:element name="Blast4-value">
  <xs:complexType>
    <xs:choice>
<!-- scalar types -->
      <xs:element name="Blast4-value_big-integer" type="xs:long"/>
      <xs:element name="Blast4-value_bioseq">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Bioseq"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_boolean">
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true"/>
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_cutoff">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Blast4-cutoff"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Blast4-value_integer" type="xs:integer"/>
