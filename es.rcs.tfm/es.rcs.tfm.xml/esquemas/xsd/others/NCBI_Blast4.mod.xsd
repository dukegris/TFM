      <xs:element name="Blast4-value_seq-align-set">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Seq-align-set"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!--
 Intended to represent user-provided masking locations for a single query
 sequence (name field in Blast4-parameter should be "LCaseMask").
 Multiple Blast4-parameters of this type are needed to specify masking
 locations for multiple queries.
-->
      <xs:element name="Blast4-value_query-mask">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Blast4-mask"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
