<!-- Complete set of simple Blast results -->
<xs:element name="Blast4-simple-results">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Blast4-simple-results_all-alignments">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Blast4-alignments-for-query"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- Alignments for one query, compiled from the raw SeqAlign results -->
<xs:element name="Blast4-alignments-for-query">
  <xs:complexType>
    <xs:sequence>
<!--
 Query sequence identifier
 (present if query is not a local id in the SeqAlign)
-->
      <xs:element name="Blast4-alignments-for-query_query-id" type="xs:string"/>
<!-- All the alignments for this query -->
      <xs:element name="Blast4-alignments-for-query_alignments">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="Blast4-simple-alignment"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- A single alignment -->
<xs:element name="Blast4-simple-alignment">
  <xs:complexType>
    <xs:sequence>
<!--
 Subject sequence identifier
 (normally a GI from the SeqAlign)
-->
      <xs:element name="Blast4-simple-alignment_subject-id" type="xs:string"/>
<!-- E-Value -->
      <xs:element name="Blast4-simple-alignment_e-value" type="xs:double"/>
<!-- Bit score -->
      <xs:element name="Blast4-simple-alignment_bit-score" type="xs:double"/>
<!-- Number of identities -->
      <xs:element name="Blast4-simple-alignment_num-identities" type="xs:integer" minOccurs="0"/>
<!-- Number of insertions/deletions -->
      <xs:element name="Blast4-simple-alignment_num-indels" type="xs:integer" minOccurs="0"/>
<!-- Full query range covered by this HSP -->
      <xs:element name="Blast4-simple-alignment_full-query-range">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Blast4-range"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- Full subject range covered by this HSP -->
      <xs:element name="Blast4-simple-alignment_full-subject-range">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Blast4-range"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- Range on a sequence - zero offset -->
<xs:element name="Blast4-range">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Blast4-range_start" type="xs:integer" minOccurs="0"/>
      <xs:element name="Blast4-range_end" type="xs:integer" minOccurs="0"/>
<!--
 The frame of the range (absent for proteins; -1/1 for nucleotides;
 -1,-2,-3,1,2,3 for translated sequences)
-->
      <xs:element name="Blast4-range_strand" type="xs:integer" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

</xs:schema>
