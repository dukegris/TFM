          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- Comments or additional information -->
      <xs:element name="PC-AssayPanelMember_comment" minOccurs="0">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="PC-AssayPanelMember_comment_E" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- often provided for profiling assays across protein families -->
      <xs:element name="PC-AssayPanelMember_target" minOccurs="0">
