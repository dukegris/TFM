<!-- short name for this panel member  -->
      <xs:element name="PC-AssayPanelMember_name" type="xs:string" minOccurs="0"/>
<!--
 description about specifics of this panel member
 such as about cell line, or target information 
-->
      <xs:element name="PC-AssayPanelMember_description" type="xs:string" minOccurs="0"/>
