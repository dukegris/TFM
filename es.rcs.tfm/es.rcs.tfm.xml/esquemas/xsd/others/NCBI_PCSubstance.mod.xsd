  <xs:complexType>
    <xs:sequence>
<!-- Total count of non-Hydrogen (Heavy) Atoms -->
      <xs:element name="PC-Count_heavy-atom" type="xs:integer"/>
<!--
 StereoChemistry Counts
 Total count of (SP3) Chiral Atoms
-->
      <xs:element name="PC-Count_atom-chiral" type="xs:integer"/>
<!-- Total count of Defined (SP3) Chiral Atoms -->
      <xs:element name="PC-Count_atom-chiral-def" type="xs:integer"/>
<!-- Total count of Undefined (SP3) Chiral Atoms -->
      <xs:element name="PC-Count_atom-chiral-undef" type="xs:integer"/>
<!-- Total count of (SP2) Chiral Bonds -->
      <xs:element name="PC-Count_bond-chiral" type="xs:integer"/>
<!-- Total count of (SP2) Defined Chiral Bonds -->
      <xs:element name="PC-Count_bond-chiral-def" type="xs:integer"/>
<!-- Total count of (SP2) Undefined Chiral Bonds -->
      <xs:element name="PC-Count_bond-chiral-undef" type="xs:integer"/>
