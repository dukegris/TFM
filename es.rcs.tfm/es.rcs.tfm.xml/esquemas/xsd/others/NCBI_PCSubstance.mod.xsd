      <xs:element name="PC-StereoPlanar_type" minOccurs="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:integer">
              <xs:attribute name="value" use="optional">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
<!--
    planar	-  SP2 Planar StereoCenter
    cumulenic	-  Cumulenic StereoCenter
-->
                    <xs:enumeration value="planar" ncbi:intvalue="1"/>
                    <xs:enumeration value="cumulenic" ncbi:intvalue="2"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
